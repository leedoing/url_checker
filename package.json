{
  "name": "url_checker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server_w": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets_w": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:server_w": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "build:assets_w": "cd src && cross-env WEBPACK_ENV=production webpack",
    "copyAll_w": "xcopy .\\src\\static .\\build\\static\\ /s /y && xcopy .\\src\\views .\\build\\views\\ /s /y",
    "build_w": "npm run build:server_w && npm run build:assets_w && npm run copyAll_w",
    "start_w": "cross-env PRODUCTION=true pm2 start build/init.js -i 0 --name 'we-tube'",
    "prebuild_w": "rd /s /q build",
    "prestart_w": "npm run build_w",
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:server": "babel src --out-dir build",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true pm2 start build/init.js -i 0 --name 'we-tube'",
    "prebuild": "rm -rf build",
    "prestart": "npm run build",
    "tunnel": "ngrok http 4000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leedoing/url_checker.git"
  },
  "author": "lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leedoing/url_checker/issues"
  },
  "homepage": "https://github.com/leedoing/url_checker#readme",
  "dependencies": {
    "autoprefixer": "^9.5.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "connect-dynamodb": "^2.0.3",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "css-loader": "^2.1.1",
    "curl": "^0.1.4",
    "dotenv": "^7.0.0",
    "dynamodb-store": "^1.0.8",
    "dynamoose": "^1.7.2",
    "email-existence": "^0.1.6",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs": "^0.0.1-security",
    "helmet": "^3.16.0",
    "mongoose": "^5.4.22",
    "morgan": "^1.9.1",
    "net": "^1.0.2",
    "node-sass": "^4.11.0",
    "nodemailer": "^6.1.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-cognito": "^0.1.13",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "pbkdf2-password": "^1.2.1",
